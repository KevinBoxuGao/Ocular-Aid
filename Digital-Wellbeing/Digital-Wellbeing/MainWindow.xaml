<Window x:Class="Digital_Wellbeing.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Digital_Wellbeing"
        xmlns:tb="http://www.hardcodet.net/taskbar"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <tb:TaskbarIcon x:Name="notifyIcon" Visibility="visible"/>
        <Frame x:Name="Main" Margin="0.3" NavigationUIVisibility="Hidden"/>
        <Grid Margin="10,10,592,181">
            <Button Height="64" Width="64" Margin="0,0,-1,0" Background="#00000000" BorderBrush="#00707070" VerticalAlignment="Top" HorizontalAlignment="Left">
                <Image Source="Icons/menu.png" Stretch="Fill"/>
            </Button>
        </Grid>
    </Grid>
</Window>
